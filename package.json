{
    "type": "module",
    "scripts": {
        "start": "tsc --build --verbose && node dist/index.js",
        "test": "tsc --build && node dist/test.js",
        "build:esbuild": "esbuild build.ts --bundle --outfile=build.js --platform=node --format=esm --packages=external && node build.js",
        "build:rolldown": "rolldown -c rolldown.config.mjs",
        "deps-check": "taze major",
        "deploy": "tsc --build && node dist/deploy.ts"
    },
    "dependencies": {
        "@prisma/client": "^6.19.0",
        "bufferutil": "^4.0.9",
        "discord.js": "^14.25.0",
        "dotenv": "^17.2.3",
        "lru-cache": "^11.2.2",
        "openai": "^6.9.1",
        "pino": "^10.1.0"
    },
    "optionalDependencies": {
        "@antfu/eslint-config": "^6.2.0",
        "eslint": "^9.39.1",
        "postject": "^1.0.0-alpha.6"
    },
    "devDependencies": {
        "@prisma/extension-accelerate": "^2.0.2",
        "esbuild": "^0.27.0",
        "pino-pretty": "^13.1.2",
        "prisma": "^6.19.0",
        "rolldown": "^1.0.0-beta.51",
        "taze": "^19.9.0",
        "typescript": "^5.9.3"
    }
}
