{
    "type": "module",
    "scripts": {
        "start": "tsc --build --verbose && node dist/index.js",
        "test": "tsc --build && node dist/test.js",
        "dev": "npm run build:esbuild --no-bundle && node bundle.cjs | pino-pretty -c",
        "build:esbuild": "esbuild build.ts --bundle --outfile=build.js --platform=node --format=esm --packages=external && node build.js",
        "build:rolldown": "rolldown -c rolldown.config.mjs",
        "deps-check": "taze major",
        "deploy": "tsc --build && node dist/deploy.ts",
        "generate-db": "prisma generate"
    },
    "dependencies": {
        "@prisma/adapter-pg": "^7.1.0",
        "@prisma/client": "^7.1.0",
        "bufferutil": "^4.0.9",
        "discord.js": "^14.25.1",
        "dotenv": "^17.2.3",
        "lru-cache": "^11.2.4",
        "openai": "^6.10.0",
        "pino": "^10.1.0"
    },
    "optionalDependencies": {
        "@antfu/eslint-config": "^6.6.1",
        "eslint": "^9.39.2",
        "postject": "^1.0.0-alpha.6"
    },
    "devDependencies": {
        "@prisma/extension-accelerate": "^3.0.1",
        "esbuild": "^0.27.1",
        "pino-pretty": "^13.1.3",
        "prisma": "^7.1.0",
        "rolldown": "^1.0.0-beta.54",
        "taze": "^19.9.2",
        "typescript": "^5.9.3"
    }
}
